{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "cypress/globals": true
  },
  "extends": ["plugin:react/recommended", "airbnb","prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "cypress"],
  "rules": { 
    "quotes" : "off",
    "no-alert":"off",
    "react/jsx-no-useless-fragment":"off"
    
  }
}
